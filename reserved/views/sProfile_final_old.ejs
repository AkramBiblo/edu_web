                                    let fgp = data.first_term_allMarks[4][0]
                                    let st = data.second_term_allMarks;
                                    let slg = data.second_term_allMarks[3][0]
                                    let sgp = data.second_term_allMarks[4][0]
                                    $('#marksheet').empty()
