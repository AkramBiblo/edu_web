                                        let fullM = FM[i]
                                        let passM = PM[i]
                                        let s = subjects[i]
                                        let FTR = f[0][i]
                                        let STR = st[0][i]
                                        let FLG = flg[i]
                                        let FGP = fgp[i]
                                        let SLG = slg[i]
                                        let SGP = sgp[i]
